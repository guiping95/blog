<div class="col-lg-12" style="text-align: center;">
    <div >
        <% if (!isFirstPage) { %>
        <span class="prepage" style="background-color: #f8f8f8;"><a style="color:#2aabd2;" title="上一页" href="?p=<%= page-1 %>">上一页</a></span>
        <% } %>

        <% if (!isLastPage) { %>
        <span class="nextpage" style="background-color: #f8f8f8;border-radius: 5px;"><a style="color: #2aabd2;" title="下一页" href="?p=<%= page+1 %>">下一页</a></span>
        <% } %>
    </div>
</div>
